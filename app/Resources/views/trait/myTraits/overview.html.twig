{% extends 'base.navigation.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    {% if app.user %}
        <script src="{{ asset('assets/js/trait/myTraits.js') }}"></script>
    {% endif %}
{% endblock %}

{% block content %}
    {% if app.user %}
        {% if dbversion in dbversions_for_user_trait_upload %}
            {% include 'trait/myTraits/upload.panel.html.twig' %}
            {% include 'trait/myTraits/overview.table.html.twig' %}
        {% else %}
            This dbversion does not support upload of traits by users.
        {% endif %}
    {% else %}
        Please log in to see your projects
    {% endif %}
{% endblock %}